//New interface Driveable
package com.company;

public interface Driveable {

    void drive(int speed);

    String brake();
}

//New class Car
package com.company;

public class Car implements Driveable {

    public Car() {
    }

    @Override
    public void drive(int speed) {
        System.out.println("The Car drives " + speed + " km/h");
    }

    @Override
    public String brake() {
        return "The Car stopped.";
    }
}

//New class Train
package com.company;

public class Train implements Driveable {

    public Train() {
    }

    @Override
    public void drive(int speed) {
        System.out.println("The Train drives " + speed + " km/h");
    }

    @Override
    public String brake() {
        return "The Train stopped.";
    }
}

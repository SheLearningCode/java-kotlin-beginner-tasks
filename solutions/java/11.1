//New class Fly
package com.company;

public class Fly implements Flyable {

    public Fly() {
    }

    @Override
    public void currentLocation(double x, double y) {
        System.out.println("Altitude " + x + " / Width degree " + y);
    }

    @Override
    public void speed(int x) {
        System.out.println("Current speed is " + x + "km/h");
    }

    @Override
    public void destination(String country) {
        System.out.println(country);
    }
}

//New interface Flyable
package com.company;

public interface Flyable {

    void currentLocation(double x, double y);

    void speed(int x);

    void destination(String country);
}


//Main class
package com.company;

public class Main {

    public static void main(String[] args) {
	    // write your code here

        Fly fly = new Fly();

        fly.currentLocation(13.2, 34.3);
        fly.speed(140);
        fly.destination("France");
    }
}
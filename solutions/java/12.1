//New class Person
package com.company;

public class Person {
    private String name;
    private int age;
    private int employeeNo;

    public Person(String name, int age, int employeeNo) {
        this.name = name;
        this.age = age;
        this.employeeNo = employeeNo;
    }

    public String getName() {
        return this.name;
    }

    public int getAge() {
        return this.age;
    }

    public int getEmployeeNo() {
        return this.employeeNo;
    }

    public String toString() {
        return "\n Name: " + this.getName() + "\n Age: " + this.getAge() + "\n Employee number: " + this.getEmployeeNo();
    }
}
